{
    "name": "mittwald/mstudio-ext-bundle",
    "description": "Symfony bundle for receiving mittwald mStudio webhook calls and managing extension state",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mittwald\\MStudio\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mittwald\\MStudio\\Bundle\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Martin Helmich",
            "email": "m.helmich@mittwald.de"
        }
    ],
    "require": {
      "mittwald/mstudio-ext-webhooks": "^0.1",
      "mittwald/mstudio-ext-auth": "^0.1",
      "symfony/framework-bundle": "^7.1",
      "symfony/security-bundle": "^7.1",
      "symfony/psr-http-message-bridge": "^7.1",
      "symfony/event-dispatcher-contracts": "^3.5",
      "symfony/uid": "^7.1",
      "doctrine/orm": "^3",
      "doctrine/doctrine-bundle": "^2",
      "ext-openssl": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^12.4"
    }
}
